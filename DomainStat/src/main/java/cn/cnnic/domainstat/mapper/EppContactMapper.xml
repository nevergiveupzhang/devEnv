<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cnnic.domainstat.mapper.EppContactMapper">
	<select id="query" resultType="cn.cnnic.domainstat.po.EppContactPO">
		select CONTACT_ID,ADDR_PC,ADDR_SP,ADDR_STREET from EPP_CONTACT_VIEW
		<where>
			<if test="contactId!=null">contact_id=#{contactId}</if>
		</where> 
	</select>
</mapper>