<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cnnic.domainstat.mapper.EppEntDomainMapper">
	<resultMap type="cn.cnnic.domainstat.po.CDomainPO" id="eppEntDomainMap">
		<result property="domainName" column="DOMAIN_NAME" javaType="string" jdbcType="VARCHAR"/>
		<result property="associateSerial" column="ASSOCIATE_SERIAL" javaType="string" jdbcType="VARCHAR"/>
		<result property="sponsorRegrid" column="sponsor_regrid" javaType="string" jdbcType="VARCHAR"/>
		<result property="regDt" column="REG_DT" javaType="string" jdbcType="DATE"/>
		<result property="registrantId" column="REGISTRANT_ID" javaType="string" jdbcType="VARCHAR"/>
	</resultMap>
	<select id="query" resultMap="eppEntDomainMap">
		select DOMAIN_NAME,ASSOCIATE_SERIAL,SPONSOR_REDRID,REG_DT,REGISTRANT_ID from EPP_ENT_ALL_DOMAIN_VIEW
		<where>
			REGISTRAR_ID &lt;&gt; 'CNNIC'
			<if test="null!=startDate">CREATE_TIME &gt;= to_date(#{startDate},'yyyy-mm-dd')</if>
			<if test="null!=endDate"> and CREATE_TIME &lt;= to_date(#{endDate},'yyyy-mm-dd')</if>
		</where> 
	</select>
</mapper>