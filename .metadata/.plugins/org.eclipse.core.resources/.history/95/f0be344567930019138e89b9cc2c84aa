package cn.cnnic.report.config;

import java.util.List;

import org.springframework.boot.context.properties.ConfigurationProperties;
import org.springframework.stereotype.Component;

import cn.cnnic.report.config.model.IndexConfigModel;
@Component
@ConfigurationProperties(prefix="report")
public class ReportApplicationModel {
	private List<IndexConfigModel> indexes;

	public List<IndexConfigModel> getIndexes() {
		return indexes;
	}

	public void setIndexes(List<IndexConfigModel> indexes) {
		this.indexes = indexes;
	}
}
